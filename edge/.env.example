# Edge Device Configuration
# Copy this to .env and fill in your values

# Cloud Connection
CLOUD_URL=https://your-cloud-server.com
EDGE_ID=edge_xxxxxxxx
EDGE_SECRET=your-secret-from-cloud-dashboard

# Detection Settings
DETECTION_MODEL=yolo11n.pt
DETECTION_CONFIDENCE=0.5
DETECTION_FPS=5

# Storage
RECORDING_RETENTION_DAYS=7

# Optional: GPU settings (for NVIDIA GPUs)
# CUDA_VISIBLE_DEVICES=0
